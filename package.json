    "@types/md5": "^2.3.5",
    "@types/node": "^24.0.4",
    "@typescript-eslint/utils": "^8.34.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vitest/ui": "^3.2.4",
