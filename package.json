{
  "name": "nativemind-extension",
  "version": "1.6.5",
  "private": false,
  "author": "NativeMind",
  "keywords": [
    "browser-extension",
    "ai-assistant",
    "cloud-ai",
    "openrouter",
    "chatgpt",
    "claude",
    "gemini",
    "gpt-4",
    "translation",
    "writing-tools",
    "summarization",
    "web-extension",
    "chrome-extension",
    "firefox-addon",
    "ai-chat",
    "productivity",
    "native-mind"
  ],
  "license": "AGPL-3.0-only",
  "description": "NativeMind â€“ Privacy-focused browser extension that connects you to leading AI models (OpenRouter, OpenAI, Claude, Gemini) with advanced writing tools, translation, and smart browsing features.",
  "repository": {
    "type": "git",
    "url": "https://github.com/NativeMindBrowser/NativeMindExtension"
  },
  "bugs": {
    "url": "https://github.com/NativeMindBrowser/NativeMindExtension/issues"
  },
  "homepage": "https://github.com/NativeMindBrowser/NativeMindExtension#readme",
  "sideEffects": [
    "./utils/**/*"
  ],
  "type": "module",
  "scripts": {
    "build:beta": "wxt build -c wxt.config.beta.ts",
    "build:beta:firefox": "wxt build -c wxt.config.beta.ts -b firefox --mv3",
    "build:prod": "wxt build",
    "build:prod:firefox": "wxt build -b firefox --mv3",
    "build:analyze": "wxt build --analyze",
    "build:analyze:firefox": "wxt build -b firefox --mv3 --analyze",
    "check": "vue-tsc --noEmit && eslint",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "compile": "vue-tsc --noEmit",
    "test:e2e": "playwright test --config=playwright.config.ts",
    "test:unit": "vitest run",
    "dev": "wxt",
    "dev:edge": "wxt -b edge",
    "dev:firefox": "wxt -b firefox --mv3",
    "postinstall": "wxt prepare",
    "release:beta": "release-it --preRelease=beta --config .release-it.beta.json",
    "release:prod": "release-it",
    "release:patch": "release-it patch",
    "zip:beta": "wxt zip -c wxt.config.beta.ts",
    "zip:beta:edge": "wxt zip -c wxt.config.beta.ts -b edge",
    "zip:beta:firefox": "wxt zip -c wxt.config.beta.ts -b firefox --mv3",
    "zip:prod": "wxt zip",
    "zip:prod:firefox": "wxt zip -b firefox --mv3",
    "prepare": "husky"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
